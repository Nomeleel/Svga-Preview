{
  "name": "svga-preview",
  "displayName": "svga-preview",
  "description": "Svga preview extension for VS Code.",
  "version": "0.1.0",
  "publisher": "Nomeleel",
  "homepage": "https://github.com/Nomeleel",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nomeleel/Svga-Preview"
  },
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:svga.preview"
  ],
  "main": "./out/extension.js",
  "icon": "resource/icon/svga.png",
  "contributes": {
    "customEditors": [
      {
        "viewType": "svga.preview",
        "displayName": "Svga Preview",
        "selector": [
          {
            "filenamePattern": "*.svga"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "publishwebview": "webpack --mode production --config ./src/webview/webpack.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "svga.lite": "^1.6.1",
    "typescript": "^4.9.4",
    "vscode-test": "^1.3.0",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0"
  }
}
