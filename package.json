{
	"name": "svga-preview",
	"displayName": "svga-preview",
	"description": "Svga preview extension for VS Code.",
	"version": "0.0.1",
	"publisher": "Nomeleel",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:svga.preview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "svga.preview",
				"displayName": "Svga Preview",
				"selector": [
					{
						"filenamePattern": "*.svga"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"publishwebview": "webpack --mode production --config ./src/webview/webpack.config.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^5.54.0",
		"webpack-cli": "^4.8.0",
		"copy-webpack-plugin": "^9.0.1",
		"babel-loader": "^8.2.2",
    "@babel/core": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0"
	}
}
